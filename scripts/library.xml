<?xml version="1.0" ?><STATES>    <MAINMENU>        <TEXTURES>            <texture filename="assets/ui/buttons/play_button.png" ID="play_button"/>            <texture filename="assets/ui/buttons/exit_button.png" ID="exit_button"/>        </TEXTURES>        <OBJECTS>            <object type="MenuButton" x="200" y="70" width="231"            height="66" textureID="play_button" numFrames="0" animSpeed = "10"            callBackID="1"/>            <object type="MenuButton" x="200" y="200" width="231"            height="66" textureID="exit_button" numFrames="0" animSpeed = "10"            callBackID="2"/>        </OBJECTS>    </MAINMENU>    <PLAY>        <TEXTURES>          <texture filename="assets/sprites/chars/walk.png"         ID="player_walk"/>          <texture filename="assets/sprites/objects/objects.png"    ID="objects"/>        </TEXTURES>        <SOUNDS>            <sound filename="assets/sounds/jump.wav"    ID="jump_sound"/>            <sound filename="assets/sounds/jetpack.wav" ID="jet_sound"/>            <sound filename="assets/sounds/land.wav"    ID="land_sound"/>        </SOUNDS>        <SONGS>            <song filename="assets/songs/runngun.ogg" ID="level_song"/>        </SONGS>        <LEVELS>          <level filename = "levels/Level0-uncompressed.tmx" ID="level0"/>          <level filename = "levels/Level1-uncompressed.tmx" ID="level1"/>          <level filename = "levels/Level2-uncompressed.tmx" ID="level2"/>          <level filename = "levels/Level3-uncompressed.tmx" ID="level3"/>          <level filename = "levels/Level4-uncompressed.tmx" ID="level4"/>          <level filename = "levels/Level5-uncompressed.tmx" ID="level5"/>        </LEVELS>		<ELEMENTS>            <element name ="Player" type="Player" textureID="player_walk"            width="32" height="32" animSpeed = "10" numFrames="4"            row ="1" frame ="0"            effectID="add_gold" soundID="gold_sound"/>            <element name ="Treasure" type="Enemy" textureID="objects"            width="32" height="32" numFrames="8" animSpeed="10"            row ="2" frame ="5" effectID="add_gold" soundID="gold_sound"/>            <element name ="Flame" type="Enemy" textureID="objects"            width="32" height="32" row ="7" frame ="9" numFrames="2"            animSpeed="10" effectID="hurt_player"            effectAmount="999"soundID="hurt_sound"/>                              </ELEMENTS>    </PLAY>    <PAUSE>        <TEXTURES>            <texture filename="assets/ui/buttons/resume_button.png" ID="resume_button"/>            <texture filename="assets/ui/buttons/menu_button.png" ID="menu_button"/>        </TEXTURES>        <OBJECTS>            <object type="MenuButton" x="200" y="70" width="231"            height="66" textureID="resume_button" numFrames="0" animSpeed = "10"            callBackID="1"/>            <object type="MenuButton" x="200" y="200" width="231"            height="66" textureID="menu_button" numFrames="1" animSpeed = "10"            callBackID="2"/>        </OBJECTS>    </PAUSE>    <GAMEOVER>        <TEXTURES>            <texture filename="assets/ui/gfx/game_over.png" ID="game_over"/>            <texture filename="assets/ui/buttons/restart_button.png" ID="restart_button"/>        </TEXTURES>        <OBJECTS>            <object type="AnimatedGraphic" x="200" y="70" width="600"            height="66" textureID="game_over" numFrames="2" animSpeed = "5"            callBackID="1"/>            <object type="MenuButton" x="90" y="200" width="231"            height="66" textureID="restart_button" numFrames="0" animSpeed = "10"            callBackID="1"/>        </OBJECTS>    </GAMEOVER></STATES>