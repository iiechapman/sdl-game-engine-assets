"0"/>            <element name ="HUD" type="Enemy" textureID="objects"            width="32" height="32" numFrames="8" animSpeed="10"            row ="2" frame ="5" effectID="add_gold" soundID="gold_sound"/>            <element name ="Flame" type="Enemy" textureID="objects"            width="32" height="32" row ="7" frame ="9" numFrames="2"            animSpeed="8" effectID="hurt_player"            effectAmount="999"soundID="hurt_sound"/>            <element name ="Crystal" type="Enemy" textureID="objects"            width="32" height="32" row ="1" frame ="3" numFrames="5"            animSpeed="20" effectID="hurt_player"            effectAmount="999"soundID="hurt_sound"/>            <element name ="Light" type="Light" textureID="light"            width="195" height="195" row ="1" frame ="0" numFrames="1"            animSpeed="0" effectID="hurt_player"            effectAmount="999"soundID="hurt_sound"/>                    </ELEMENTS>    </PLAY>    <PAUSE>        <TEXTURES>            <texture filename="res/art/ui/btn/resume_button.png" ID="resume_button"/>            <texture filename="res/art/ui/btn/menu_button.png" ID="menu_button"/>        </TEXTURES>        <OBJECTS>            <object type="MenuButton" x="200" y="70" width="231"            height="66" textureID="resume_button" numFrames="0" animSpeed = "10"            callBackID="1"/>            <object type="MenuButton" x="200" y="200" width="231"            height="66" textureID="menu_button" numFrames="1" animSpeed = "10"            callBackID="2"/>        </OBJECTS>    </PAUSE>    <GAMEOVER>        <TEXTURES>            <texture filename="res/art/ui/gfx/game_over.png" ID="game_over"/>            <texture filename="res/art/ui/buttons/restart_button.png" ID="restart_button"/>        </TEXTURES>        <OBJECTS>            <object type="AnimatedGraphic" x="200" y="70" width="600"            height="66" textureID="game_over" numFrames="2" animSpeed = "5"       